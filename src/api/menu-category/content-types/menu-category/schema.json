{
  "kind": "collectionType",
  "collectionName": "menu_categories",
  "info": {
    "singularName": "menu-category",
    "pluralName": "menu-categories",
    "displayName": "menu category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "sortOrder": {
      "type": "biginteger"
    },
    "is_active": {
      "type": "boolean"
    },
    "menu_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu-item.menu-item",
      "mappedBy": "category"
    }
  }
}
